{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Labeling",
            "type": "python",
            "request": "launch",
            "module": "labeling.main",
            "justMyCode": true,
            "args": ["best.pt"]
        },
        {
            "name": "Train",
            "type": "python",
            "request": "launch",
            "module": "src.train.train",
            "justMyCode": false
        },
        {
            "name": "Show data",
            "type": "python",
            "request": "launch",
            "module": "src.visualization.show_dataset",
            "justMyCode": false,
            "args": ["--ckpt", "epoch=799-step=48000.ckpt", "--yolo", "best.pt"]
        },
        {
            "name": "Export ONNX",
            "type": "python",
            "request": "launch",
            "module": "src.train.export_onnx",
            "justMyCode": true,
            "args": ["epoch=799-step=48000.ckpt", "model.onnx"]
        },
        {
            "name": "Run ONNX",
            "type": "python",
            "request": "launch",
            "module": "src.visualization.run_onnx",
            "justMyCode": true,
            "args": ["--model", "mobilenetv3_120_160_model.onnx"]
        },
        {
            "name": "Run Openvino",
            "type": "python",
            "request": "launch",
            "module": "src.visualization.run_openvino",
            "justMyCode": true,
            "args": ["--model", "mobilenetv3_120_160.xml", "--image", "datasets/sample_images/sample.png"]
        },
        {
            "name": "Run",
            "type": "python",
            "request": "launch",
            "module": "src.visualization.run",
            "justMyCode": true,
            "args": ["--model", "mobilenetv3_120_160.xml", "--runtime", "openvino", "--image", "datasets/sample_images/sample.png"]
        }
    ]
}